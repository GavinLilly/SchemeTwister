{
  "name": "legendizer",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "class-to-json": "ts-node --project tools/tsconfig.tools.json tools/scripts/classToJSON.ts",
    "commit": "git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/platform-express": "^6.8.3",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@ngrx/store": "^9.2.0",
    "@nrwl/angular": "9.4.5",
    "bootstrap": "^4.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.4",
    "uuid": "^8.2.0",
    "zone.js": "^0.10.2",
    "@ngrx/effects": "9.1.0",
    "@ngrx/entity": "9.1.0",
    "@ngrx/router-store": "9.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.3",
    "@ngrx/schematics": "^9.2.0",
    "@nrwl/cypress": "9.4.5",
    "@nrwl/jest": "9.4.5",
    "@nrwl/nest": "9.4.5",
    "@nrwl/node": "9.4.5",
    "@nrwl/workspace": "9.4.5",
    "@semantic-release/exec": "^5.0.0",
    "@types/jest": "25.1.4",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "commitiquette": "^1.0.4",
    "commitizen": "^4.0.3",
    "conventional-changelog-conventionalcommits": "^4.3.0",
    "cypress": "^4.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "prettier": "2.0.4",
    "semantic-release": "^17.1.1",
    "semantic-release-docker": "^2.2.0",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3",
    "@ngrx/store-devtools": "9.1.0"
  },
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator"
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "docker build --file Dockerfile.angular -t gavtherav/legendizer ."
        }
      ],
      [
        "semantic-release-docker",
        {
          "name": "gavtherav/legendizer"
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
