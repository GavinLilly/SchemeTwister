{
  "name": "schemetwister",
  "version": "2.3.2",
  "workspaces": [
    "libtwister",
    "web-app"
  ],
  "scripts": {
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
