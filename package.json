  "scripts": {
    "ng": "nx",
    "postinstall": "npm run build-version",
    "postversion": "npm run build-version",
    "nx": "nx",
    "start": "nx serve",
    "startdev": "nx serve --configuration local",
    "build": "npm run build-version && nx build",
    "build-prod": "npm run build-version && nx build --prod",
    "test": "nx run-many --all --target=test --update-snapshot",
