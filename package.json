    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-ordered-imports": "^0.6.0",
    "firebase-tools": "^8.0.0 || ^9.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^7.0.4",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^27.4.3",
    "jest-preset-angular": "11.1.1",
    "jsonc-parser": "^3.0.0",
