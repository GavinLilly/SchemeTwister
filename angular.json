            "passWithNoTests": true,
            "codeCoverage": true
          },
          "outputs": ["coverage/libs/web-app/feature-home"]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "web-app-feature-randomize": {
      "projectType": "library",
      "root": "libs/web-app/feature-randomize",
      "sourceRoot": "libs/web-app/feature-randomize/src",
      "prefix": "schemetwister",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web-app/feature-randomize/src/**/*.ts",
              "libs/web-app/feature-randomize/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/web-app/feature-randomize/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          },
          "outputs": ["coverage/libs/web-app/feature-randomize"]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "web-app-feature-store": {
      "projectType": "library",
      "root": "libs/web-app/feature-store",
      "sourceRoot": "libs/web-app/feature-store/src",
      "prefix": "schemetwister",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web-app/feature-store"],
          "options": {
            "jestConfig": "libs/web-app/feature-store/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web-app/feature-store/src/**/*.ts",
              "libs/web-app/feature-store/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-app-feature-view": {
      "projectType": "library",
      "root": "libs/web-app/feature-view",
      "sourceRoot": "libs/web-app/feature-view/src",
      "prefix": "schemetwister",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web-app/feature-view"],
          "options": {
            "jestConfig": "libs/web-app/feature-view/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web-app/feature-view/src/**/*.ts",
              "libs/web-app/feature-view/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-app-shared": {
      "projectType": "library",
      "root": "libs/web-app/shared",
      "sourceRoot": "libs/web-app/shared/src",
      "prefix": "schemetwister",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web-app/shared"],
          "options": {
            "jestConfig": "libs/web-app/shared/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web-app/shared/src/**/*.ts",
              "libs/web-app/shared/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-app-ui": {
      "projectType": "library",
      "root": "libs/web-app/ui",
      "sourceRoot": "libs/web-app/ui/src",
      "prefix": "schemetwister",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web-app/ui/src/**/*.ts",
              "libs/web-app/ui/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/web-app/ui/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          },
          "outputs": ["coverage/libs/web-app/ui"]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {}
  },
  "defaultProject": "web-app"
}
