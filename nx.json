    "web-app-e2e": {
      "tags": []
    },
    "web-app-feature-home": {
      "tags": ["type:feature", "scope:web-app"]
    },
    "web-app-feature-randomize": {
      "tags": ["type:feature", "scope:web-app"]
    },
    "web-app-ui": {
      "tags": ["type:ui", "scope:web-app"]
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
